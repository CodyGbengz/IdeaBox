{
  "name": "ideabox",
  "version": "1.0.0",
  "description": "a web platform for sharing ideas",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production babel-node server/server.js",
    "start:dev": "nodemon --exec babel-node server/server.js",
    "start:client": "NODE_ENV=development babel-node server/server.js",
    "build": "webpack --config webpack.prod.js",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha -c --compilers js:babel-register server/tests/** --timeout 50000 --exit",
    "test:client": "jest --coverage",
    "coverage": "nyc npm test --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "engines": {
    "node": "8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillyWunderdog/IdeaBox.git"
  },
  "author": "gbenga ode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WillyWunderdog/IdeaBox/issues"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "server/tests/",
      "server/server.js",
      "server/config/**"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "homepage": "https://github.com/WillyWunderdog/IdeaBox#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.10",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "fetch-mock": "^5.12.2",
    "html-webpack-plugin": "^2.30.1",
    "jsonwebtoken": "^8.1.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.5",
    "mocha": "^5.0.1",
    "moment": "^2.21.0",
    "mongoose": "^5.0.6",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "path": "^0.12.7",
    "prop-types": "^15.6.1",
    "react": "^15.6.2",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.7",
    "react-router": "^3.0.2",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "validator": "^9.4.1",
    "validatorjs": "^3.14.2",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "expect": "^22.4.0",
    "jest": "^22.4.2",
    "moxios": "^0.4.0",
    "nodemon": "^1.15.1",
    "nyc": "^11.4.1",
    "react-test-renderer": "^15.6.2",
    "redux-devtools": "^3.4.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.1"
  }
}
