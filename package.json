{
  "name": "ideabox",
  "version": "1.0.0",
  "description": "a web platform for sharing ideas",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --exec babel-node server/server.js",
    "start:client": "webpack-dev-server --config webpack.config.js",
    "build": "webpack --config webpack.prod.js",
    "test": "nyc --reporter=html --reporter=text mocha -c --compilers js:babel-register  --timeout 15000 server/tests/**",
    "coverage": "nyc npm test --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillyWunderdog/IdeaBox.git"
  },
  "author": "gbenga ode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WillyWunderdog/IdeaBox/issues"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "tests/"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "homepage": "https://github.com/WillyWunderdog/IdeaBox#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "clean-webpack-plugin": "^0.1.18",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^5.0.1",
    "mongoose": "^5.0.6",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.15.1",
    "nyc": "^11.4.1"
  }
}
